<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jobfinder">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/BaseTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Main.SignInActivity" />
        <activity android:name=".Home.HomeActivity" />
        <activity android:name=".Main.SignupActivity" />
        <activity android:name=".Searcher.SignInSearcher" />
        <activity android:name=".Worker.WorkerSignup" />
        <activity android:name=".Worker.WorkerSignin"></activity>
    </application>

</manifest>